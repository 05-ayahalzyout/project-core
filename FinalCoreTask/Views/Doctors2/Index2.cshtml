@model IEnumerable<FinalCoreTask.Models.Doctors2>

@{
    ViewData["Title"] = "Index2";
}

<h1>Index2</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DoctorName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Clinic)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DoctorName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Clinic.Id)
            </td>

        </tr>
}
    </tbody>
</table>
